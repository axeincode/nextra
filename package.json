{
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build types",
    "types": "turbo run types",
    "dev:core": "turbo run dev --scope=\"nextra\"",
    "dev:theme-blog": "turbo run dev --scope=\"blog\" --include-dependencies",
    "dev:theme-docs": "turbo run dev --scope=\"swr-site\" --include-dependencies",
    "build:core": "pnpm run build --filter nextra",
    "build:theme-blog": "pnpm run build --filter nextra-theme-blog",
    "build:theme-docs": "pnpm run build --filter nextra-theme-docs",
    "version-beta": "lerna version --preid beta",
    "publish-beta": "lerna publish from-package --canary --preid beta --pre-dist-tag beta",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.0",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "concurrently": "^7.0.0",
    "cssnano": "^4.1.10",
    "esbuild": "^0.14.0",
    "next": "^12.1.0",
    "postcss": "^8.4.5",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^3.0.16",
    "turbo": "^1.1.3",
    "typescript": "^4.5.5",
    "vitest": "^0.4.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "packageManager": "pnpm@6.27.1",
  "turbo": {
    "baseBranch": "origin/core",
    "npmClient": "pnpm",
    "pipeline": {
      "build": {
        "dependsOn": [
          "types",
          "^build"
        ],
        "outputs": [
          "dist/**/*.js",
          "dist/**/*.css",
          ".next/**",
          "style.css",
          "index.js",
          "ssg.js",
          "loader.js"
        ]
      },
      "test": {},
      "types": {
        "outputs": [
          "dist/**/*.d.ts"
        ]
      },
      "dev": {
        "cache": false
      }
    }
  }
}
