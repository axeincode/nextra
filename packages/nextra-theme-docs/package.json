{
  "name": "nextra-theme-docs",
  "version": "2.0.0-beta.10",
  "description": "A Nextra theme for documentation sites.",
  "repository": "https://github.com/shuding/nextra",
  "author": "Shu Ding <g@shud.in>",
  "license": "MIT",
  "files": [
    "dist",
    "style.css"
  ],
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "callout": [
        "./dist/callout.d.ts"
      ],
      "bleed": [
        "./dist/bleed.d.ts"
      ],
      "tabs": [
        "./dist/tabs.d.ts"
      ],
      "collapse": [
        "./dist/collapse.d.ts"
      ]
    }
  },
  "types": "./dist/index.d.ts",
  "exports": {
    "./style.css": "./style.css",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./callout": {
      "import": "./dist/callout.js",
      "types": "./dist/callout.d.ts"
    },
    "./bleed": {
      "import": "./dist/bleed.js",
      "types": "./dist/bleed.d.ts"
    },
    "./tabs": {
      "import": "./dist/tabs.js",
      "types": "./dist/tabs.d.ts"
    },
    "./collapse": {
      "import": "./dist/collapse.js",
      "types": "./dist/collapse.d.ts"
    }
  },
  "scripts": {
    "dev": "concurrently \"pnpm dev:layout\" \"pnpm dev:tailwind\"",
    "build": "tsup",
    "build:tailwind": "cross-env NODE_ENV=production pnpm postcss src/styles.css -o style.css --verbose",
    "types": "tsup --dts-only",
    "dev:layout": "tsup --watch",
    "dev:tailwind": "cross-env TAILWIND_MODE=watch pnpm postcss src/styles.css -o style.css --watch",
    "prepublishOnly": "pnpm build",
    "test": "vitest --run",
    "clean": "rimraf ./dist ./style.css"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.5",
    "@mdx-js/react": "^2.1.2",
    "@reach/skip-nav": "^0.17.0",
    "classnames": "^2.3.1",
    "flexsearch": "^0.7.21",
    "focus-visible": "^5.2.0",
    "github-slugger": "^1.4.0",
    "intersection-observer": "^0.12.2",
    "match-sorter": "^4.2.0",
    "next-themes": "^0.2.0-beta.2",
    "parse-git-url": "^1.0.1",
    "scroll-into-view-if-needed": "^2.2.29",
    "title": "^3.4.2"
  },
  "peerDependencies": {
    "next": ">=9.5.3",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "devDependencies": {
    "@types/flexsearch": "^0.7.2",
    "@types/react": "^17.0.39",
    "autoprefixer": "^10.2.6",
    "cross-env": "^7.0.2",
    "nextra": "workspace:*",
    "tsup": "^6.1.3"
  }
}