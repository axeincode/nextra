{
  "name": "nextra",
  "version": "2.0.0-beta.10",
  "description": "Next.js and MDX based site generator.",
  "files": [
    "dist/*",
    "icons/*",
    "loader.js"
  ],
  "types": "./dist/types.d.ts",
  "main": "./dist/index.js",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.ts"
      ],
      "compile": [
        "./dist/compile.d.ts"
      ],
      "context": [
        "./dist/context.d.ts"
      ],
      "data": [
        "./dist/ssg.d.ts"
      ],
      "ssg": [
        "./dist/ssg.d.ts"
      ],
      "locales": [
        "./dist/locales.d.ts"
      ],
      "icons": [
        "./dist/icons.d.ts"
      ]
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./compile": {
      "import": "./dist/compile.mjs",
      "types": "./dist/compile.d.ts"
    },
    "./context": {
      "import": "./dist/context.js",
      "types": "./dist/context.d.ts"
    },
    "./data": {
      "import": "./dist/ssg.js",
      "types": "./dist/ssg.d.ts"
    },
    "./ssg": {
      "import": "./dist/ssg.js",
      "types": "./dist/ssg.d.ts"
    },
    "./loader": "./loader.js",
    "./locales": {
      "import": "./dist/locales.js",
      "types": "./dist/locales.d.ts"
    },
    "./icons": {
      "import": "./dist/icons.mjs",
      "types": "./dist/icons.d.ts"
    }
  },
  "repository": "https://github.com/shuding/nextra",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "types": "tsup --dts-only",
    "test": "vitest run",
    "prepublishOnly": "pnpm build",
    "clean": "rimraf ./dist ./style.css"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.1.2",
    "@napi-rs/simple-git": "^0.1.8",
    "github-slugger": "^1.4.0",
    "graceful-fs": "^4.2.10",
    "gray-matter": "^4.0.3",
    "rehype-mdx-title": "^1.0.0",
    "rehype-pretty-code": "0.2.4",
    "remark-gfm": "^3.0.1",
    "shiki": "0.10.1",
    "slash": "^3.0.0"
  },
  "peerDependencies": {
    "react": ">=16.13.1"
  },
  "devDependencies": {
    "@types/graceful-fs": "^4.1.5",
    "@types/mdast": "^3.0.10",
    "@types/webpack": "^5.28.0",
    "tsup": "^6.1.3"
  }
}